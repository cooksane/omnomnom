<div id="instruction_{{index}}" class="row instruction semantic-selected" data-index="{{index}}">
    <div class="col-lg-1 col-xs-1 list-number">
        <p>{{index}}.</p>
    </div>
    <div class="col-lg-11 col-xs-11">
        <p class="semantic-summary">{{summary}}</p>
        <p class="semantic-text">{{text}}</p>
    </div>
</div>